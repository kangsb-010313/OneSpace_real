    <script>
	$(document).ready(function() {

	    // ÌéòÏù¥ÏßÄ Î°úÎî© Ïãú, Í∞Å ÌõÑÎ≥¥Ïùò Ìà¨ÌëúÏûê Î™©Î°ùÏùÑ Í∞ÄÏ†∏ÏòµÎãàÎã§.
	    $(".voter-list").each(function() {
	        var voteNo = $(this).attr("id").replace("voter-list-", "");
	        if (voteNo) { fetchVoters(voteNo); }
	    });
	
	    // 'Ìà¨Ìëú' Î≤ÑÌäº ÌÅ¥Î¶≠ Ïù¥Î≤§Ìä∏
	    $(".btn-vote").on("click", function() {
	        var $button = $(this); // ÌÅ¥Î¶≠Îêú Î≤ÑÌäº ÏöîÏÜåÎ•º Î≥ÄÏàòÏóê Ï†ÄÏû•
	        var voteNo = $button.data("voteno");
	        var postNo = "${post.teamPostNo}";

            // (ÌïµÏã¨) Ïù¥ÎØ∏ 'active' ÌÅ¥ÎûòÏä§Í∞Ä ÏûàÎã§Î©¥ (Ï¶â, Ïù¥ÎØ∏ Ìà¨ÌëúÌñàÎã§Î©¥) ÏïÑÎ¨¥Í≤ÉÎèÑ ÌïòÏßÄ ÏïäÏùå
            if ($button.hasClass('active')) {
                // Ïó¨Í∏∞ÏÑú ÎÇòÏ§ëÏóê 'Ìà¨Ìëú Ï∑®ÏÜå' Î°úÏßÅÏùÑ ÎÑ£ÏùÑ Ïàò ÏûàÏäµÎãàÎã§.
                // alert("Ïù¥ÎØ∏ Ìà¨ÌëúÌïú ÌõÑÎ≥¥ÏûÖÎãàÎã§.");
                return;
            }

	        $.ajax({
	            url: "${pageContext.request.contextPath}/onespace/api/addvote",
	            type: "POST", 
                data: { voteNo: voteNo, postNo: postNo },
	            success: function(result) {
	                if (result) {
	                    // ÏÑ±Í≥µ Ïãú, Î≤ÑÌäºÏóê 'active' ÌÅ¥ÎûòÏä§ Ï∂îÍ∞ÄÌïòÏó¨ Î≥¥ÎùºÏÉâÏúºÎ°ú ÎßåÎì¶
                        $button.addClass('active');
	                    // Ìà¨ÌëúÏûê Î™©Î°ù Ïã§ÏãúÍ∞Ñ Í∞±Ïã†
	                    fetchVoters(voteNo);
	                } else {
	                    // ÏÑúÎ≤ÑÏóêÏÑú falseÎ•º Î∞òÌôòÌïú Í≤ΩÏö∞ (Ïù¥ÎØ∏ Îã§Î•∏ ÌõÑÎ≥¥ÏóêÍ≤å Ìà¨ÌëúÌñàÍ±∞ÎÇò Îì±)
	                    alert("Ïù¥ÎØ∏ Ìà¨ÌëúÌïòÏÖ®Í±∞ÎÇò Ïò§Î•òÍ∞Ä Î∞úÏÉùÌñàÏäµÎãàÎã§.");
	                }
	            },
	            error: function() { 
                    alert("Ìà¨Ìëú Ï≤òÎ¶¨ Ï§ë Ïò§Î•òÍ∞Ä Î∞úÏÉùÌñàÏäµÎãàÎã§."); 
                }
	        });
	    });
	
	    // Ìà¨ÌëúÏûê Î™©Î°ùÏùÑ Í∞ÄÏ†∏ÏôÄÏÑú ÌôîÎ©¥Ïóê ÎøåÎ†§Ï£ºÎäî Ìï®Ïàò
	    function fetchVoters(voteNo) {
	        $.ajax({
	            url: "${pageContext.request.contextPath}/onespace/api/getvoters",
	            type: "GET", 
                data: { voteNo: voteNo },
	            success: function(voterList) {
	                var voterListDiv = $("#voter-list-" + voteNo);
	                voterListDiv.empty();
